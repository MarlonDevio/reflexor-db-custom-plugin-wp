{
  "name": "marlondev/fp-reflexor-dashboard",
  "description": "reflexor dashboard",
  "license": "GPLv3",
  "autoload": {
    "psr-4": {
      "FP\\": "app/"
    }
  },
  "authors": [
    {
      "name": "Marlon Hamaekers",
      "email": "mar.hamaekers@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/view": "^10.15",
    "illuminate/support": "^10.15"
  },
  "scripts": {
    "lint": "phpcs",
    "format": "phpcbf"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "wp-coding-standards/wpcs": "^3.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
