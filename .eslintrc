{
  "root": true,
  "extends": [
    "semistandard",
    "prettier"
  ],
  "rules": {
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "curly": [
      "error",
      "multi-line"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "padding-line-between-statements": [
      "error",
      {
        "blankLine": "always",
        "prev": "*",
        "next": "return"
      }
    ]
  }
}
